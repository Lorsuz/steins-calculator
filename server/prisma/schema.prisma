datasource db {
  provider = "sqlite"
  url      = "file:../database/database.sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    Int     @id @default(autoincrement())
  username String  @unique
  email String  @unique
  password  String
  profile  String @default("default.png")
  background  String @default("default.jpg")
}

